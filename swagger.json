    "paths": {
        "/v1/signup": {
            "post": {
                "tags": [
                    "User"
                ],
                "summary": "Rota de criação de um usuario",
                "requestBody": {
                    "description": "Informações para criação de usuario",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/user"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Usuario Criado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/ResponseCreated"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Informaçoes erradas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/BadRequest"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Usuario já cadastrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "message": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erro inesperado no servidor",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "message": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/signin": {
            "post": {
                "tags": [
                    "Session"
                ],
                "summary": "Rota para Logar o usuario",
                "requestBody": {
                    "description": "Informações para criação de usuario",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/RequestLogin"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Usuario Logado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/ResponseCreated"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Informaçoes erradas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/BadRequest"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Usuario e/ou senha invalidas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/user/{id}": {
            "get": {
                "tags": [
                    "User"
                ],
                "summary": "Buscar usuario",
                "parameters": [
                    {
                        "in": "path",
                        "name": "uuid",
                        "schema": {
                            "type": "string"
                        },
                        "required": true,
                        "description": "O uuid é uma string que representa o usuario"
                    },
                    {
                        "in": "header",
                        "name": "Bearer token",
                        "schema": {
                            "type": "string"
                        },
                        "required": true,
                        "description": "O Bearer Token, é um token jwt que determina se o requisitante tem autorização ou não de acessar aquela rota"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Usuario",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/ResponseGetUser"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Não encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Não autorizado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "User"
                ],
                "summary": "Rota de atualização de um usuario",
                "parameters": [
                    {
                        "in": "header",
                        "name": "Bearer token",
                        "schema": {
                            "type": "string"
                        },
                        "required": true,
                        "description": "O Bearer Token, é um token jwt que determina se o requisitante tem autorização ou não de acessar aquela rota"
                    },
                    {
                        "in": "header",
                        "name": "Bearer token",
                        "schema": {
                            "type": "string"
                        },
                        "required": true,
                        "description": "O Bearer Token, é um token jwt que determina se o requisitante tem autorização ou não de acessar aquela rota"
                    }
                ],
                "requestBody": {
                    "description": "Informações que atualização o usuario",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/user"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Usuario Atualzado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "n": {
                                            "type": "integer"
                                        },
                                        "nModified": {
                                            "type": "integer"
                                        },
                                        "ok": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Informaçoes erradas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/BadRequest"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Não autorizado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "User"
                ],
                "summary": "Rota de delete de um usuario",
                "parameters": [
                    {
                        "in": "header",
                        "name": "Bearer token",
                        "schema": {
                            "type": "string"
                        },
                        "required": true,
                        "description": "O Bearer Token, é um token jwt que determina se o requisitante tem autorização ou não de acessar aquela rota"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Usuario apagado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "n": {
                                            "type": "integer"
                                        },
                                        "ok": {
                                            "type": "integer"
                                        },
                                        "deletedCount": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Não autorizado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/health": {
            "get": {
                "tags": [
                    "Health"
                ],
                "summary": "Rota para checar se a aplicacao esta de pe",
                "responses": {
