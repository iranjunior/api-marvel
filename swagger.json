            "get": {
                "tags": [
                    "Health"
                ],
                "summary": "Rota para checar se ha comunicacao estavel com a api da marvel",
                "responses": {
                    "200": {
                        "description": "Marvel retornando dados normal",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
